{
	"name": "debuggy/gauger",
	"type": "library",
	"description": "Very extensible tool for measuring and debugging PHP scripts and applications",
	"keywords": ["timer", "profiler", "benchmark", "gauge", "debug"],
	"license": "Apache-2.0",
	"authors": [{"name": "Latyntsev Sergey"}],
	"require": {
		"php": ">=5.3.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Debuggy\\": "src/Debuggy/"
		}
	},
	"archive": {
		"exclude": ["vendor", "tests", "phpunit.xml", ".travis.yml", "composer.phar", "*coveralls.yml"]
	}
}
