{
	"name": "debuggy/gauger",
	"type": "library",
	"description": "Comprehensive timing and gauging tool for PHP",
	"keywords": ["time", "gauge", "debug"],
	"license": "Apache License, Version 2.0",
	"authors": [{"name": "Latyntsev Sergey", "email": "dnsl48@gmail.com"}],
	"require": {
		"php": ">=5.3.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"satooshi/php-coveralls": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Debuggy\\": "src/"
		}
	},
	"archive": {
		"exclude": ["vendor", "tests", "phpunit.xml", ".travis.yml", "composer.phar", "*coveralls.yml"]
	}
}
